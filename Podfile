# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'Poketto' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Alamofire', '4.8.0'
  pod 'BiometricAuthentication'
  pod 'Chester'
  pod 'SwiftKeychainWrapper'
  pod 'SwiftyJSON', '4.0'
  pod 'web3swift', :git => 'https://github.com/pokettocash/web3swift.git'
  pod 'Presentr'
  pod 'QRCodeReader.swift', '~> 10.0.0'
  pod 'SVProgressHUD'
  pod 'MagicalRecord', :git => 'https://github.com/magicalpanda/MagicalRecord.git', :tag => 'v2.3.3'
  pod 'PocketSwift/Core', '~> 0.0.2'


  target 'PokettoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PokettoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
